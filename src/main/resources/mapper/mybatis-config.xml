	</settings>
	
    <typeAliases>
        <typeAlias alias="UserVO" type="com.daekyo.clab.user.UserVO" />
    </typeAliases>	

	<mappers>
		<mapper resource="mapper/user/User_SQL.xml" />
	</mappers>
</configuration>