<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user">
    
	<select id="selectLogin" resultType="UserVO" parameterType="UserVO">
		<![CDATA[
		SELECT * FROM TB_VIDEO_MEETING_USER WHERE USER_ID = #{userId} AND USER_PW = #{userPw} AND ENABLED = '1'
 		]]>
	</select>
	
	<select id="selectUser" resultType="UserVO" parameterType="String">
		<![CDATA[
		SELECT * FROM TB_VIDEO_MEETING_USER WHERE USER_ID = #{userId}
 		]]>
	</select>
</mapper>